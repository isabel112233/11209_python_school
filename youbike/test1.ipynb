    "import time\n",
    "import threading \n",
    "\n",
    "def countdown(n:int)->None:\n",
    "    while n>0:\n",
    "        print(f\"倒數計時:{n}\")\n",
    "        n -= 1\n",
    "        time.sleep(5)\n",
    "#countdown(10)\n",
    "t = threading.Thread(target=countdown,args=(10,))  #tuple()如果只有一個值要加逗號 \n",
    "t.start()"
   ]
  }
